class POOKL {
  carnage: any;
  constructor(brand) {
    this.carnage = brand;
  }
  present() {
    return 'I have a ' + this.carnage;
  }
}

class Model extends POOKL {
  model: any;
  constructor(brand, mod) {
    super(brand);
    this.model = mod;
  }
  show() {
    return this.present() + ', it is a ' + this.model;
  }
}

let myCar = new Model("Ford", "Mustang");
const newLocal = document.getElementById("demo");
